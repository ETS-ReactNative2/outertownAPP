{
  "expo": {
    "name": "Outer Town Fest",
    "description": "Everything you need to know about Bristol's Outer Town Fest 2022",
    "slug": "outertownAPP",
    "version": "1.0.0",
    "orientation": "portrait",
    "privacy": "public",
    "backgroundColor": "#f4f4ec",
    "icon": "./assets/graphics/icon.png",
    "notification": {
      "iosDisplayInForeground": true
    },
    "androidStatusBar": {
      "barStyle": "dark-content"
    },
    "splash": {
      "image": "./assets/graphics/header-bldgs.png",
      "resizeMode": "contain",
      "backgroundColor": "#f4f4ec"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "assets/*"
    ],
    "ios": {
      "bundleIdentifier": "com.nigelpowell.outertownapp",
      "buildNumber": "1.0.0",
      "supportsTablet": false,
      "requireFullScreen": true,
      "backgroundColor": "#f4f4ec",
      "icon": "./assets/graphics/icon.png",
      "userInterfaceStyle": "dark"
    },
    "android": {
      "package": "com.nigelpowell.outertownapp",
      "versionCode": 1,
      "icon": "./assets/graphics/icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/graphics/iconAdaptiveForeground.png",
        "backgroundColor": "#f4f4ec"
      },
      "permissions" : [
        "ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/graphics/favicons/favicon.ico"
    }
  }
}
